    "puppeteer-core": "^13.5.2",
    "shiki": "0.9.4",
    "vue": "^3.2.25",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.14",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@vercel/node": "^1.14.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@windicss/plugin-animations": "^1.0.9",
    "shx": "^0.3.4",
    "typescript": "^4.5.4",
    "unplugin-icons": "^0.13.3",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.0",
    "vite-plugin-pages": "^0.21.4",
